<div class="container" id="home-cont">
  <h1>WELCOME WORLD</h1>
    <% if @current_user.present? %>
    <h1>
      Welcome <%= @current_user.email %>
    </h1>
    <% end %>

    <div class="sign-up">
        <%= link_to "Sign Up ?", new_user_path %>
    </div>

    <div class="login">
        <%= link_to "Login ?", login_path %>
    </div>
</div>





